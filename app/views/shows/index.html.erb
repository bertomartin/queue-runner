<div id="show-list">
  <% @watches.each do |show, watches| %>
    <div class="row-fluid">
      <div class="span12">
        <div class="show-table">
          <div class="row-fluid">
            <div class="span10">
              <h2><%= show.name %></h2>
            </div>
          </div>
        <%= render :partial => "shows/watches", :locals => { :watches => watches } %>
        </div>
      </div>
    </div>
  <% end %>
</div>